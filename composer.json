{
    "name": "mink/webdriver-classic-driver",
    "description": "W3C WebDriver Classic driver for Mink framework",
    "keywords": ["selenium", "webdriver", "javascript", "ajax", "testing", "browser"],
    "homepage": "https://mink.behat.org/",
    "type": "mink-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Sciberras",
            "email": "christian@sciberras.me",
            "homepage": "https://github.com/uuf6429"
        }
    ],
    "require": {
        "php": ">=7.4",
        "behat/mink": "^1.9@dev"
    },
    "require-dev": {
        "mink/driver-testsuite": "dev-master",
        "phpunit/phpunit": "^9.6.8",
        "symfony/error-handler": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Mink\\Driver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mink\\Tests\\Driver\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
